  author     = {Zamboni, Giulia A. and Cappello, Giovanni and Caruso, Damiano and Gourtsoyianni, Sofia and Cyran, Clemens and Schlemmer, Heinz-Peter and D'Anastasi, Melvin and Fournier, Laure and Neri, Emanuele},
  year       = 2024,
