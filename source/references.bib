@article{schaddeLimitationsResectabilityColorectal2021,
  title    = {Limitations in Resectability of Colorectal Liver Metastases 2020 - {{A}} Systematic Approach for Clinicians and Patients},
  author   = {Schadde, Erik and Grunhagen, Dirk J and Verhoef, Cornelis and Krzywon, Lucyna and Metrakos, Peter},
