@article{valderrama-trevinoHepaticMetastasisColorectal2017,
  title    = {Hepatic {{Metastasis}} from {{Colorectal Cancer}}},
  author   = {{Valderrama-Trevi{\~n}o}, Alan I and {Barrera-Mera}, Baltazar and {Ceballos-Villalva}, Jes{\'u}s C and {Montalvo-Jav{\'e}}, Eduardo E},
  year     = 2017,
